#LAYERS

layerToken-s
	[[a-zA-Z_][a-zA-Z0-9_]*] as NAME
	[.*] as WILD
	"\n" as NEWLINE
	"layers" as LAYERS
layerBrace-s are Braced
	[,.] as LAYER_FILE with WILD as layer_file_name 
layerImporter-s are Import
	[><<\"gui/\",layer_file_name,\".layers\">>\n] as IMPORT_LAYER from (NEWLINE|COMMENTS|layer_declaration)*; with LAYER_FILE LAYERS
layer_declaration
	NAME as layerName 
